      if (sc & CC_V) exec = 1;
      else           exec = 0;
      break;
    case 7:  /* VC (V clear) */
      if (sc & CC_V) exec = 0;
      else           exec = 1;
      break;
    case 8:  /* HI (C=0&Z=0) */
