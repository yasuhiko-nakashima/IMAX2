      else                                                                                  exec = 0;
      break;
    case 13: /* LE (Z=1|N!=V */
      if (!(sc & CC_Z) && (((sc & CC_N) && (sc & CC_V)) || (!(sc & CC_N) && !(sc & CC_V)))) exec = 0;
      else                                                                                  exec = 1;
      break;
    case 14: /* AL (Always) */
      exec = 1;
      break;
    case 15: /* AL (Always) */
      exec = 1;
      break;
    }
