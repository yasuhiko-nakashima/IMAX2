  Ull  s1, s2, s3, sc, t0, m0; /* source, through, middle(sft,shifter-carry), destination(alu) */
d29 8
d296 4
a299 8
    if (rob->dbl) {
      top = (Sll)s2>>63;
      D0 = (top & ~tmask)|(bot & tmask);
    }
    else {
      top = (Sll)((Ull)s2<<32)>>63;
      D0 = ((top & ~tmask)|(bot & tmask)) & 0x00000000ffffffffLL;
